{
    "compilerOptions": {
      "composite": true,
      "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
      "skipLibCheck": true,
      "module": "ESNext",
      "moduleResolution": "bundler",
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "noEmit": true
    },
    "include": ["vite.config.ts", "declarations.d.ts", "src/**/*.ts", "src/**/*.d.ts", "vite-env.d.ts"],
    "exclude": [
        "node_modules"
    ],
  }